{
  "name": "@tscircuit/schematic-corpus",
  "version": "0.0.3",
  "main": "./dist/bundled-bpc-graphs.json",
  "devDependencies": {
    "@biomejs/biome": "^2.0.0",
    "@types/bun": "latest",
    "bun-match-svg": "^0.0.11",
    "circuit-json-to-bpc": "^0.0.1",
    "tscircuit": "^0.0.505",
    "@tscircuit/cli": "^0.1.146",
    "@tscircuit/core": "^0.0.507",
    "@tscircuit/props": "^0.0.239"
  },
  "files": [
    "dist"
  ],
  "exports": {
